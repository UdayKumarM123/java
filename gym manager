import java.util.Scanner;
public class p1 {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("welcome to gym management:");
        System.out.println("enter the name:");
        String name=sc.nextLine();
        System.out.println("enter the age:");
        int age=sc.nextInt();
        System.out.println("membership is active or not");
        boolean isActive=sc.nextBoolean();
        System.out.println("enter the duration:");
        int duration=sc.nextInt();

        System.out.println("membership fee is:");
        double fee=duration*50.0;

        System.out.println("name:"+ name);
        System.out.println("age:"+ age);
        System.out.println("membership Active:" + isActive);
        System.out.println("duration is:" + duration);
        System.out.println("membership fee is:" + fee);
        sc.close();

    }


}
