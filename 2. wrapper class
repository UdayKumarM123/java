public class p3 {
    public static void main(String[] arg){
        // boxing
        Integer boxedInt=Integer.valueOf(10);
        Double boxedDouble=Double.valueOf(20.3);
        Float boxedFloat=Float.valueOf(3.14f);
        Boolean boxedBoolean=Boolean.valueOf(true);
        Character boxedChar=Character.valueOf('A');

        //unboxing
        int unboxedInt=boxedInt.intValue();
        double unboxedDouble=boxedDouble.doubleValue();
        float unboxedFloat=boxedFloat.floatValue();
        boolean unboxedBoolean=boxedBoolean.booleanValue();
        char unboxedChar=boxedChar.charValue();


        System.out.println("boxedInt:" + boxedInt);
        System.out.println("unboxedInt:" + unboxedInt);
        System.out.println("boxedDouble:" + boxedDouble);
        System.out.println("unboxedFloat:" + unboxedFloat);
        System.out.println("boxedBoolean:" + boxedBoolean);
        System.out.println("unboxedBoolean:" + unboxedBoolean);
        System.out.println("boxedChar:" + boxedChar);
        System.out.println("unboxedChar:" + unboxedChar);

    }
}
