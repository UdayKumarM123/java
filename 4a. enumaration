import java.util.Scanner;

public class p6 {
    public enum DayofWeek{
        SUNDAY,
        MONDAY,
        TUESDAY,
        WEDNESDAY,
        THURSDAY,
        FRIDAY,
        SATURDAY;

        public Boolean isworkday(){
            return (this!=SATURDAY && this!=SUNDAY);
    }
    }
    public static void main(String[] arg){
        System.out.println("saturday working:" + DayofWeek.SATURDAY.isworkday());
        System.out.println("monday working:" + DayofWeek.MONDAY.isworkday());

    }
}

